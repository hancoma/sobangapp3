<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sobangdcmart.hancoma" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>소방DC마트.com</name>
    <description>소방DC마트</description>
    <author email="ch7@daum.net" href="http://sobangdcmart.cafe24.com">김대경</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <access launch-external="true" origin="tel:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="pgb-builder-version" value="2" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="fullscreen" value="false" />
    <preference name="android-windowSoftInputMode" value="adjustResize" />
    <preference name="android-build-tool" value="gradle" />
</widget>
